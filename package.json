{
  "name": "learning-react-ts",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://brunojensen.github.io/learning-react-ts",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.22",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "2.5.1",
    "react-scripts": "5.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@giphy/js-fetch-api": "^4.1.2",
    "@giphy/react-components": "^5.4.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.4.0",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "gh-pages": "^3.2.3",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-moment": "^1.1.1",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "yarn prettier --list-different './src/**/*.{js,ts,tsx}'",
    "lint-write": "prettier --write .",
    "dockerize": "yarn install && yarn build && docker build -t $npm_package_name:$npm_package_version .",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
